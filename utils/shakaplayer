import ShakaPlayer from 'shaka-player';

const VideoPlayer = () => {
  useEffect(() => {
    const video = document.getElementById('video');
    const player = new ShakaPlayer(video);
    
    player.load("https://storage.googleapis.com/dugmlobjectracking/video_chunks/output/manifest.m3u8");
  }, []);

  return <video id="video" controls></video>;
};
